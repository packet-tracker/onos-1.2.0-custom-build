<?xml version="1.0" encoding="UTF-8"?><components xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0">
    <scr:component immediate="true" name="org.onosproject.acl.impl.DistributedAclStore" activate="activate" deactivate="deactive">
        <implementation class="org.onosproject.acl.impl.DistributedAclStore"/>
        <service servicefactory="false">
            <provide interface="org.onosproject.acl.AclStore"/>
            <provide interface="org.onosproject.store.Store"/>
        </service>
        <property name="service.pid" value="org.onosproject.acl.impl.DistributedAclStore"/>
        <reference name="storageService" interface="org.onosproject.store.service.StorageService" cardinality="1..1" policy="static" bind="bindStorageService" unbind="unbindStorageService"/>
        <reference name="coreService" interface="org.onosproject.core.CoreService" cardinality="1..1" policy="static" bind="bindCoreService" unbind="unbindCoreService"/>
    </scr:component>
</components>
